{
  "name": "mysodexo",
  "version": "2019.12.08",
  "description": "Client for Mysodexo API",
  "main": "src/index.js",
  "bin": {
    "mysodexo": "src/cli.js"
  },
  "files": [
    "src/sodexows.mo2o.com_client-android.*.pem",
    "src/*.js"
  ],
  "scripts": {
    "test": "jest --coverage src/*test.js",
    "test:coveralls": "jest --coverage && coveralls < coverage/lcov.info",
    "lint": "eslint src/*.js",
    "test:inspect": "node --inspect node_modules/.bin/jest --runInBand src/*.test.js",
    "test:debug": "node debug node_modules/.bin/jest --runInBand src/*.test.js",
    "run:api": "node src/index.js",
    "run:api:inspect": "node --inspect src/index.js",
    "run:api:debug": "node debug src/index.js",
    "run:cli": "node cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/AndreMiras/mysodexojs.git"
  },
  "keywords": [
    "mysodexo"
  ],
  "author": "Andre Miras",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AndreMiras/mysodexojs/issues"
  },
  "homepage": "https://github.com/AndreMiras/mysodexojs",
  "devDependencies": {
    "eslint-plugin-jest": "^23.0.4",
    "jest": "^24.9.0",
    "rewire": "^4.0.1"
  },
  "dependencies": {
    "coveralls": "^3.0.8",
    "read": "^1.0.7",
    "request": "^2.88.0"
  }
}
